<mapper namespace="com.nzc.hrm.dao.UserInfoDao" >

   <insert id="insertItem" parameterType="userInfo">
       insert into user_inf(LOGINNAME,PASSWORD,USERNAME,CREATEDATE) values(#{loginName},#{password},#{username},#{createDate})
   </insert>
</mapper>